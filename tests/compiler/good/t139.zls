let g() returns (o)
 forward resume (42)[i] returns (oi out o) do
  forward resume (36)[j] returns (oij out oi) do
    do oij = 1.0 done
  done
 done

let hybrid g() returns (o)
 foreach (42)[i] returns (oi out o) do
