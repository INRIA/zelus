let f x =
  let rec f<<n>>(x) =
    match size n with
    | 0 -> 0
    | 1 -> 1
    | n -> f<<n/2>>(x-1) + f<<n/2>>(x+1) in
  f<<6>>(x)

let f3 x =
  let rec f3<<n>>() = match size n with | 0 -> 0 | n -> f3<<n-1>>() + 1 in
  f3<<2>>()

let f4 =
  let rec or_<<n>> = fun (x) returns (o)
    match size n with
    | 0 -> o = false
    | 1 -> o = true
    | _ -> 
      do o = or_<<n/2>>(x) || or_<<n/2>>(x) done in
  or_<<4>> 0