(* Robot get keyword *)


let hybrid main () = () where 
  rec trigger = (period (1.))
   and automaton 
	| First -> do t = robot_get ("transverse_vel") until trigger then Second
	| Second -> do t = robot_get ("angular_vel") until trigger then First
   end
  and ()= present(trigger) -> (print_int 0; (print_float t);print_newline ()(*; print_float s*); robot_store("transverse_vel", 30.))
  else ()
  
  
  
  
  
  
  
 
