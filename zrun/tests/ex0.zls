let node f(x) returns (y)
    do y = (0 fby x) + 1 done

let node g() returns (y, z)
  do y = f(z) and z = y done
    

let node main() returns (o)
  local m default 0, half, r
  do
    if half then do done
    else do m = 42 -> pre m + 1 done
  and
      o = m
  and
      half = true -> not (pre half)
  and
      r = m
  done
      
