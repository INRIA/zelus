type t = A | B | C | D

let node is_A(x) returns (ok)
    do ok = if x = A then true else false done

let node select(x) returns (o init 0)
  match x with
  | A -> do o = last o + 1 done
  | B -> do o = last o - 1 done
  | C -> do o = 0 done
  | D -> do o = 42 done
  end

let node main() returns (y)
  local x
  do
    x = A fby A fby B fby C fby D fby x
  and
    y = run select(x)
  done
